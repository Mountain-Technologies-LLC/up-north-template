{
  "name": "up-north-template",
  "version": "0.0.0",
  "description": "Node package for the repository as a whole",
  "scripts": {
    "build": "npm run build --if-present --prefix ./template-app -- --output-path ../dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "synth": "cdk synth --app \"dotnet run --project infrastructure/src/Infrastructure/Infrastructure.csproj\"",
    "diff": "cdk diff --app \"dotnet run --project infrastructure/src/Infrastructure/Infrastructure.csproj\"",
    "deploy": "cdk deploy --app \"dotnet run --project infrastructure/src/Infrastructure/Infrastructure.csproj\"",
    "destroy": "cdk destroy --app \"dotnet run --project infrastructure/src/Infrastructure/Infrastructure.csproj\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mountain-Technologies-LLC/up-north-template.git"
